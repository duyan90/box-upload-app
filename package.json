  "scripts": {
    "start": "nx run-many -t serve -p backend frontend --parallel",
    "start:backend": "nx serve backend",
    "start:frontend": "nx serve frontend",
    "start:api": "nx serve backend",
    "start:web": "nx serve frontend",
    "start:prod": "node dist/backend/main.js",
    "build": "nx run-many -t build -p backend frontend",
    "build:backend": "nx build backend --configuration=production",
    "build:frontend": "nx build frontend --configuration=production",
    "build:prod": "npm run build:backend && npm run build:frontend",
    "test": "nx run-many -t test -p backend frontend",
    "test:backend": "nx test backend",
    "test:frontend": "nx test frontend",
    "lint": "nx run-many -t lint -p backend frontend",
    "lint:backend": "nx lint backend",
    "lint:frontend": "nx lint frontend",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "clean": "nx reset && rm -rf .angular node_modules/.cache",
    "clean:all": "nx reset && rm -rf .angular node_modules/.cache dist"
  },
